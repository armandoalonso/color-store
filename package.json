{
  "devDependencies": {
    "adm-zip": "^0.5.16",
    "chalk": "^5.4.1",
    "child_process": "^1.0.2",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "vite": "^6.0.7",
    "webpack-cli": "^6.0.1",
    "yocto-spinner": "^0.1.2"
